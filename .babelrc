{
  "presets": [
    [
      "@babel/preset-react",
      {}
    ]
  ],
  "plugins": [
    [
      "babel-plugin-root-import",
      {
        "paths": [
          {
            "rootPathSuffix": "./src",
            "rootPathPrefix": "~/"
          },
          {
            "rootPathSuffix": "./src/components",
            "rootPathPrefix": "#/"
          }
        ]
      }
    ]
  ]
}